{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [false],
        "no-console": [false],
        "quotemark": [true, "single"],
        "eofline": true,
        "ban-ts-ignore": true,
        "no-empty": true,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "no-unused-variable": false,
        "variable-name": [true, "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}
